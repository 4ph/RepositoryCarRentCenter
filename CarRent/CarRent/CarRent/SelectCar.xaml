        Title="SelectCar" Height="311" Width="543">
    <Grid>
        <DataGrid x:Name="DataGridSelectCar" HorizontalAlignment="Left" VerticalAlignment="Top" Height="209" Width="535"/>
        <Button x:Name="ButtonSelect" Content="Select" HorizontalAlignment="Left" Margin="40,232,0,0" VerticalAlignment="Top" Width="75" Click="ButtonSelect_Click"/>
        <Button x:Name="ButtonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="153,232,0,0" VerticalAlignment="Top" Width="75" Click="ButtonCancel_Click"/>

    </Grid>
</Window>
